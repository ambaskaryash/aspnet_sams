                                            <input type="text" disabled class="form-control validator" ng-model="Employee.PreviousCompany" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div style="margin-top:5px" class="row">
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        <label class="col-sm-5 control-label" style="float:left">COUNTRY</label>
                                        <div class="col-sm-7" style="float:left">
                                            <select class="form-control" disabled ng-model="Employee.CountryId" ng-options="c.Id as c.Name for c in Employee.CountryList">
                                                <option value="">Select...</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
