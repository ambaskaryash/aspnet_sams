                            <div style="margin-top:5px" class="row">
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        <label class="col-sm-5 control-label" style="float:left">EMAIL ID</label>
                                        <div class="col-sm-7" style="float:left">
                                            <input type="text" disabled class="form-control validator" ng-model="Employee.EmailIDs" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div style="margin-top:5px" class="row">
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        <label class="col-sm-5 control-label" style="float:left">GRADE</label>
                                        <div class="col-sm-7" style="float:left">
                                            <input type="text" disabled class="form-control validator" ng-model="Employee.Grade" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div style="margin-top:5px" class="row">
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        <label class="col-sm-5 control-label" style="float:left">REMARKS</label>
                                        <div class="col-sm-7" style="float:left">
                                            <input type="text" disabled class="form-control validator" ng-model="Employee.Remarks" />
                                        </div>
                                    </div>
                                </div>
                            </div>
